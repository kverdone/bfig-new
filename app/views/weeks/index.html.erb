<h1>Listing weeks</h1>

<table>
  <tr>
    <th>Season Number</th>
    <th>Week Number</th>
    <th>Show</th>
    <th>Edit</th>
    <th>Destroy</th>
  </tr>

<% @weeks.each do |week| %>
  <tr>
    <td><%= week.season_id %></td>
    <td><%= week.week_number %></td>
    <td><%= link_to 'Show', week %></td>
    <td><%= link_to 'Edit', edit_week_path(week) %></td>
    <td><%= link_to 'Destroy', week, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Week', new_week_path %>
